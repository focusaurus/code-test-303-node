{
  "name": "api-303",
  "version": "1.0.0",
  "description": "REST API server",
  "main": "code/server.js",
  "dependencies": {
    "bole": "^3.0.2",
    "config3": "^1.0.3",
    "express": "^4.16.3",
    "process-title": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "5.3.0",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-formatter-comment": "2.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-promise": "3.8.0",
    "eslint-tap": "2.0.1",
    "node-dev": "3.1.3",
    "prettier": "1.14.2",
    "prettier-eslint-cli": "4.7.1",
    "supertest": "3.1.0",
    "tap": "12.0.1"
  },
  "scripts": {
    "test": "tap 'code/**/*-tap.js'",
    "start": "node .",
    "coverage": "tap --jobs-auto --coverage-report=html 'code/**/*-tap.js'",
    "format": "prettier-eslint --write 'code/**/*.js'",
    "lint": "eslint --format ./node_modules/eslint-tap code"
  },
  "nyc": {
    "exclude": [
      "**/*-tap.js"
    ],
    "watermarks": {
      "branches": [
        90,
        100
      ],
      "functions": [
        80,
        100
      ],
      "lines": [
        80,
        100
      ],
      "statements": [
        80,
        100
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/focusaurus/api-303.git"
  },
  "author": "Peter Lyons <pete@peterlyons.com> (http://peterlyons.com/)",
  "bugs": {
    "url": "https://github.com/focusaurus/api-303/issues"
  },
  "homepage": "https://github.com/focusaurus/api-303#readme"
}
